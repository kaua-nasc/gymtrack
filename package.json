{
  "name": "gymtrack",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "identity:db:generate": "bun ./node_modules/typeorm/cli.js migration:generate src/module/identity/persistence/migration/migration -d src/module/identity/persistence/typeorm-datasource.ts",
    "identity:db:migrate": "bun ./node_modules/typeorm/cli.js migration:run -d src/module/identity/persistence/typeorm-datasource.ts",
    "identity:db:drop": "bun ./node_modules/typeorm/cli.js schema:drop -d src/module/identity/persistence/typeorm-datasource.ts",
    "training-plan:db:generate": "bun ./node_modules/typeorm/cli.js migration:generate src/module/training-plan/persistence/migration/migration -d src/module/training-plan/persistence/typeorm-datasource.ts",
    "training-plan:db:migrate": "bun ./node_modules/typeorm/cli.js migration:run -d src/module/training-plan/persistence/typeorm-datasource.ts",
    "training-plan:db:drop": "bun ./node_modules/typeorm/cli.js schema:drop -d src/module/training-plan/persistence/typeorm-datasource.ts",
    "db:generate": "bun run identity:db:generate && bun run training-plan:db:generate",
    "db:setup": "bun run identity:db:migrate && bun run training-plan:db:migrate",
    "db:drop": "bun run identity:db:drop && bun run training-plan:db:drop",
    "docker:stop": "docker-compose stop",
    "docker:up": "docker-compose up --detach",
    "start:dev": "bun --watch src/main.ts",
    "start:prod": "bun dist/src/main.js",
    "test:db:drop": "dotenv -e .env.test -- npm run db:drop",
    "test:db:setup": "dotenv -e .env.test -- npm run db:setup",
    "test:e2e": "bun test --workers=1 ./src/module/*/__test__/e2e/*/*.spec.ts",
    "test:e2e:watch": "bun run test:e2e --watch",
    "test:integration": "bun --env-file=.env.test test --workers=1 ./src/module/*/__test__/integration/*/*.spec.ts",
    "test:integration:watch": "bun run test:integration --watch",
    "test:unit": "bun --env-file=.env.test test --workers=1 ./src/module/*/__test__/unit/*/*.spec.ts",
    "test:unit:watch": "npm run test:unit --watch"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/storage-blob": "^12.29.1",
    "@faker-js/faker": "^9.8.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dayjs": "^1.11.13",
    "factory.ts": "^1.4.2",
    "ioredis": "^5.7.0",
    "knex": "^3.1.0",
    "minio": "^8.0.6",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^6.0.1",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.2",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "postgres": "^3.4.7",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25",
    "typeorm-extension": "^3.7.1",
    "typeorm-transactional": "^0.5.0",
    "winston": "^3.17.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "^1.3.5",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^11.0.0",
    "factory.ts": "^1.4.2",
    "husky": "^9.1.7",
    "knex": "^3.1.0",
    "msw": "^2.12.4",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
